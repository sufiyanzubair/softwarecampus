$(function () {
    $('input[name="whatsappNo"]').on("click", function () {
        if ($(this).val() == "0") {
            $("#textboxes").show();
        } else {
            $("#textboxes").hide();
        }
    });
});


$(".closeDropUp").click(function () {
    $(".contactRight .dropdown-menu").removeClass("show");
});


//sticky header
$(window).scroll(function () {
    if ($(this).scrollTop() > 1200) {
        $(".topArrow").addClass('topArrowShow')
    } else {
        $(".topArrow").removeClass('topArrowShow')
    }
});
//sticky header
